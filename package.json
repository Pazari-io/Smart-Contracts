{
  "name": "crypto-supermarket",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "recompile": "yarn clean && npx hardhat compile",
    "script:local": "npx hardhat run --network localhost",
    "script:rinkeby": "npx hardhat run --network rinkeby",
    "script:fuji": "npx hardhat run --network fuji",
    "script:avax": "npx hardhat run --network avax",
    "test": "yarn test:dapp && yarn test:hh",
    "test:dapp": "dapp test",
    "test:hh": "npx hardhat test",
    "test:truffle": "npx truffle compile && npx truffle test",
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "lint:sol": "solhint -f table contracts/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "localnode": "hardhat node",
    "gas": "REPORT_GAS=true hardhat test",
    "coverage": "HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
    "typechain": "TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "clean": "rm -rf abi artifacts cache types",
    "lint-staged": "lint-staged",
    "check:push": "yarn format:fix && yarn lint:fix && yarn lint:sol"
  },
  "lint-staged": {
    "*.{ts,js,sol}": [
      "npx prettier --write ."
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.1",
    "chai": "^4.3.4",
    "truffle": "^5.4.26",
    "truffle-assertions": "^0.9.2"
  },
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@types/edit-json-file": "^1.6.1",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "chai": "^4.3.4",
    "chai-ethers": "^0.0.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "edit-json-file": "^1.6.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.7.1",
    "hardhat-abi-exporter": "^2.3.1",
    "hardhat-deploy": "^0.9.14",
    "hardhat-deploy-ethers": "^0.3.0-beta.11",
    "hardhat-gas-reporter": "^1.0.6",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-typechain": "^0.3.5",
    "hardhat-watcher": "^2.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.5",
    "typescript": "^4.5.4",
    "yarn-upgrade-all": "^0.5.4"
  }
